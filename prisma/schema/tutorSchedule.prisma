model TutorSchedule {
    id String @id @default(uuid())
    tutor_id String
    tutor TutorProfile @relation(fields: [tutor_id], references: [user_id])
    day_of_week DaysOfWeek 
    start_time   String
    end_time   String
    isAvailable Boolean @default(true)
    isActive Boolean @default(true)
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    bookings Bookings[]
}

enum DaysOfWeek {
    SUNDAY
    MONDAY
    TUESDAY
    WEDNESDAY
    THURSDAY
    FRIDAY
    SATURDAY
}